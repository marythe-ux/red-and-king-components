  "browserslist": [
    ">1.5%",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=16.13.0 <17",
    "yarn": "~1.22.17"
  },
  "scripts": {
    "build-storybook": "rm -rf ./docs/storybook && build-storybook -o ./docs/storybook",
    "build": "rm -rf ./dist && tsc -p ./tsconfig-build.json",
    "dist": "yarn build && cp ./package.json ./dist/package.json && cd ./dist && yarn pack",
    "lint": "eslint --max-warnings=0 'src/**/*.{ts,tsx}'",
    "prettier": "prettier  --write 'src/**/*.{ts,tsx}'",
    "publishNpm": "cd ./dist && yarn publish",
    "start": "start-storybook -p 6006",
    "storybook": "yarn start",
    "test-cover": "yarn test --coverage && mv  coverage docs/coverage",
    "test": "react-scripts test --watchAll=false"
  }
}
